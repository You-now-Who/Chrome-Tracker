{
  "manifest_version": 3,
  "name": "Chrome Tracker",
  "version": "1.0",
  "description": "Track Random stuff",



  "action": {
      "default_popup": "popup.html"
  },

  "host_permissions": ["https://*/*", "http://*/*"],

    "permissions": [
      "scripting",
      "activeTab",
      "storage"
      
    ],

  "background": {
      "service_worker": "scripts/background.js"
  },

  "content_scripts": [
      {
          "matches": ["https://*/*"],
          "js": ["scripts/content-script.js"]
      }
  ]
}